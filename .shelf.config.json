{
    "repository": {
        "name": "Quicklid",
        "description": "A Liquid-like string expression evaluator."
    },
    "entries": {
        "typedoc": {
            "name": "TypeDoc",
            "description": "TypeScript documentation generated using TypeDoc.",
            "type": "documentation",
            "configuration": {
                "directory": "generated-docs"
            }
        },
        "manifest": {
            "name": "Manifest Files",
            "description": "package.json and pnpm-lock.yaml.",
            "type": "manifest",
            "configuration": {
                "files": [
                    "package.json",
                    "pnpm-lock.yaml"
                ]
            }
        },
        "coverage": {
            "name": "Unit Test Coverage",
            "description": "Code coverage of the unit tests.",
            "type": "coverage",
            "configuration": {
                "lcovReportDirectory": "coverage/lcov-report",
                "lcovInfoFile": "coverage/lcov.info"
            }
        }
    }
}
